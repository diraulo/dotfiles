[core]
	autocrlf = input
[push]
	default = simple
[pull]
	rebase = true
[rerere]
	enabled = true
[color]
	ui = true
[http]
	postBuffer = 65536000
[pretty]
  colored = format:%Cred%h%Creset %s %Cgreen(%cr) %C(bold blue)%an%Creset
[alias]
	s = status -s
	lg = log --oneline --decorate --graph --all --pretty=colored
